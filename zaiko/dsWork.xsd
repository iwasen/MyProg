<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="dsWork" targetNamespace="http://tempuri.org/Work.xsd" elementFormDefault="qualified" attributeFormDefault="qualified" xmlns="http://tempuri.org/Work.xsd" xmlns:mstns="http://tempuri.org/Work.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="dsWork" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="在庫変更履歴">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="通し番号" msdata:AutoIncrement="true" type="xs:int" />
							<xs:element name="パーツ名称" type="xs:string" minOccurs="0" />
							<xs:element name="処理日時" type="xs:dateTime" minOccurs="0" />
							<xs:element name="入出庫区分" type="xs:string" minOccurs="0" />
							<xs:element name="数量" type="xs:int" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ピッキングリスト">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="顧客名" type="xs:string" minOccurs="0" />
							<xs:element name="納期" type="xs:dateTime" minOccurs="0" />
							<xs:element name="パーツ名称" type="xs:string" minOccurs="0" />
							<xs:element name="数量" type="xs:int" minOccurs="0" />
							<xs:element name="備考" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="欠品リスト">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="製造区分" minOccurs="0" type="xs:string"></xs:element>
							<xs:element name="パーツ名称" minOccurs="0" type="xs:string"></xs:element>
							<xs:element name="欠品数量" type="xs:int" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="発注点警告リスト">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="製造区分" type="xs:string" minOccurs="0" />
							<xs:element name="発注点警告日" type="xs:dateTime" minOccurs="0" />
							<xs:element name="パーツ名称" type="xs:string" minOccurs="0" />
							<xs:element name="発注点数量" type="xs:int" minOccurs="0" />
							<xs:element name="在庫比率" minOccurs="0" type="xs:int"></xs:element>
							<xs:element name="現在庫数" type="xs:int" minOccurs="0" />
							<xs:element name="備考" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="パーツ在庫リスト">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="パーツ種別名" type="xs:string" minOccurs="0" />
							<xs:element name="パーツ名称" type="xs:string" minOccurs="0" />
							<xs:element name="現在庫数" type="xs:int" minOccurs="0" />
							<xs:element name="備考" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="棚卸入力">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="パーツ名称" type="xs:string" minOccurs="0" />
							<xs:element name="現在庫数" type="xs:int" minOccurs="0" />
							<xs:element name="実棚卸数" type="xs:int" minOccurs="0" />
							<xs:element name="差異" type="xs:int" minOccurs="0" />
							<xs:element name="棚卸した日" type="xs:dateTime" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="受注明細">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="受注番号" type="xs:string" nillable="true" />
							<xs:element name="商品名称" type="xs:string" minOccurs="0" />
							<xs:element name="受注数量" type="xs:int" minOccurs="0" />
							<xs:element name="顧客コード" type="xs:string" minOccurs="0" />
							<xs:element name="受注日" type="xs:dateTime" minOccurs="0" />
							<xs:element name="納期" type="xs:dateTime" minOccurs="0" />
							<xs:element name="標準商品名称" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="入出庫入力">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="パーツ名称" type="xs:string" minOccurs="0" />
							<xs:element name="現在庫数" type="xs:int" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="パーツ展開">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="受注番号" type="xs:string" />
							<xs:element name="商品名称" type="xs:string" minOccurs="0" />
							<xs:element name="受注数量" type="xs:int" minOccurs="0" />
							<xs:element name="パーツ1" type="xs:string" minOccurs="0" />
							<xs:element name="数量1" type="xs:int" minOccurs="0" />
							<xs:element name="パーツ2" type="xs:string" minOccurs="0" />
							<xs:element name="数量2" type="xs:int" minOccurs="0" />
							<xs:element name="パーツ3" type="xs:string" minOccurs="0" />
							<xs:element name="数量3" type="xs:int" minOccurs="0" />
							<xs:element name="パーツ4" type="xs:string" minOccurs="0" />
							<xs:element name="数量4" type="xs:int" minOccurs="0" />
							<xs:element name="パーツ5" type="xs:string" minOccurs="0" />
							<xs:element name="数量5" type="xs:int" minOccurs="0" />
							<xs:element name="パーツ6" type="xs:string" minOccurs="0" />
							<xs:element name="数量6" type="xs:int" minOccurs="0" />
							<xs:element name="パーツ7" type="xs:string" minOccurs="0" />
							<xs:element name="数量7" type="xs:int" minOccurs="0" />
							<xs:element name="パーツ8" type="xs:string" minOccurs="0" />
							<xs:element name="数量8" type="xs:int" minOccurs="0" />
							<xs:element name="パーツ9" type="xs:string" minOccurs="0" />
							<xs:element name="数量9" type="xs:int" minOccurs="0" />
							<xs:element name="パーツ10" type="xs:string" minOccurs="0" />
							<xs:element name="数量10" type="xs:int" minOccurs="0" />
							<xs:element name="パーツ11" type="xs:string" minOccurs="0" />
							<xs:element name="数量11" type="xs:int" minOccurs="0" />
							<xs:element name="パーツ12" type="xs:string" minOccurs="0" />
							<xs:element name="数量12" type="xs:int" minOccurs="0" />
							<xs:element name="パーツ13" type="xs:string" minOccurs="0" />
							<xs:element name="数量13" type="xs:int" minOccurs="0" />
							<xs:element name="パーツ14" type="xs:string" minOccurs="0" />
							<xs:element name="数量14" type="xs:int" minOccurs="0" />
							<xs:element name="パーツ15" type="xs:string" minOccurs="0" />
							<xs:element name="数量15" type="xs:int" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="パーツ在庫引当">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="パーツ名称" type="xs:string" minOccurs="0" />
							<xs:element name="引当数" type="xs:int" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="受注ファイル">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="F1" type="xs:string" minOccurs="0" />
							<xs:element name="F2" type="xs:string" minOccurs="0" />
							<xs:element name="F3" type="xs:string" minOccurs="0" />
							<xs:element name="F4" type="xs:string" minOccurs="0" />
							<xs:element name="F5" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="月別出庫データ">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="年月" type="xs:string" minOccurs="0" />
							<xs:element name="出庫数" type="xs:int" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
		<xs:key name="dsWorkKey2" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:パーツ在庫引当" />
			<xs:field xpath="mstns:パーツ名称" />
		</xs:key>
		<xs:key name="dsWorkKey3" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:月別出庫データ" />
			<xs:field xpath="mstns:年月" />
		</xs:key>
	</xs:element>
</xs:schema>